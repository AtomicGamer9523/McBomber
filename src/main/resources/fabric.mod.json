{
  "schemaVersion": 1,
  "id": "mcbomber",
  "version": "${version}",
  "name": "McBomber",
  "description": "Makes Bombing bases easier",
  "authors": [
    "Atomic"
  ],
  "license": "MIT",
  "icon": "assets/mcbomber/icon.png",
  "environment": "client",
  "entrypoints": {
    "meteor": [
      "atomic.mcbomber.meteor.Addon"
    ]
  },
  "mixins": [
    "mcbomber.mixins.json"
  ],
  "custom": {
    "meteor-client:color": "225,25,25"
  },
  "depends": {
    "java": ">=17",
    "minecraft": ">=${mc_version}",
    "meteor-client": "*"
  }
}
